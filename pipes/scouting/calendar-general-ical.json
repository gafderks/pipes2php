{
  "pipe":{
    "title":"Calendar General",
    "modules":[
      {
        "type":"FetchICS",
        "id":1,
        "conf":{
          "URL":"https://calendar.google.com/calendar/ical/agenda%40descouting.nl/public/basic.ics",
          "title":"Verkenners",
          "months":[
            "jan", "feb", "mrt", "apr", "mei", "jun", "jul", "aug", "sep", "okt", "nov", "dec"
          ],
          "days":[
            "Ma", "Di", "Wo", "Do", "Vr", "Za", "Zo"
          ],
          "date":"D j M Y",
          "time":"G:i",
          "timezone":"Europe/Amsterdam"
        }
      },
      {
        "type":"Filter",
        "id":6,
        "conf":{
          "mode": "permit",
          "combine": "and",
          "rules":[
            {
              "field":"start",
              "op":"after",
              "value":"now"
            }
          ]
        }
      },
      {
        "type":"Sort",
        "id":2,
        "conf":{
          "orders":[
            {
              "field":"start",
              "order":"asc"
            }
          ]
        }
      },
      {
        "type":"RegEx",
        "id":3,
        "conf":{
          "rules":[
            {
              "field":"summary",
              "match":"Status van afspraak: .*",
              "replace":""
            },
            {
              "field":"summary",
              "match":"Wie: .*",
              "replace":""
            },
            {
              "field":"summary",
              "match":"Wanneer: ",
              "replace":""
            },
            {
              "field":"summary",
              "match":"Waar:",
              "replace":"<br/>Waar:"
            },
            {
              "field":"summary",
              "match":"Beschrijving van afspraak: .*",
              "replace":""
            },
            {
              "field":"summary",
              "match":"\n",
              "replace":"",
              "globalmatch":true,
              "multilinematch":true
            },
            {
              "field":"summary",
              "match":"\r",
              "replace":"",
              "globalmatch":true,
              "multilinematch":true
            },
            {
              "field":"summary",
              "match":"CET",
              "replace":"",
              "globalmatch":true
            },
            {
              "field":"summary",
              "match":"(\\d)\\.",
              "replace":"$1",
              "globalmatch":true
            },
            {
              "field":"title",
              "match":"Verk: ",
              "replace":""
            },
            {
              "field":"summary",
              "match":"CEST",
              "replace":"",
              "globalmatch":true
            },
            {
              "field":"summary",
              "match":"<br>",
              "replace":" ",
              "globalmatch":true,
              "multilinematch":true
            },
            {
              "field":"summary",
              "match":"&nbsp;",
              "replace":"",
              "globalmatch":true
            }
          ]
        }
      },
      {
        "type":"Rename",
        "id":4,
        "conf":{
          "rules":[
            {
              "field":"content",
              "op":"copy",
              "newval":"summary"
            }
          ]
        }
      },
      {
        "type":"Output",
        "id":5,
        "conf":{

        }
      }
    ],
    "wires":[
      {
        "id":0,
        "src":{
          "module":1
        },
        "tgt":{
          "module":6
        }
      },
      {
        "id":0,
        "src":{
          "module":6
        },
        "tgt":{
          "module":2
        }
      },
      {
        "id":3,
        "src":{
          "module":2
        },
        "tgt":{
          "module":3
        }
      },
      {
        "id":1,
        "src":{
          "module":3
        },
        "tgt":{
          "module":4
        }
      },
      {
        "id":2,
        "src":{
          "module":4
        },
        "tgt":{
          "module":5
        }
      }
    ]
  }
}