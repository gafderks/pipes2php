{
  "pipe":{
    "title":"Calendar General",
    "modules":[
      {
        "type":"FetchFeed",
        "id":1,
        "conf":{
          "URL":"https://www.google.com/calendar/feeds/agenda%40descouting.nl/public/basic?futureevents=true&orderby=starttime&sortorder=a&language=nl_NL&country=NL&singleevents=true&hl=nl"
        }
      },
      {
        "type":"RegEx",
        "id":2,
        "conf":{
          "rules":[
            {
              "field":"summary",
              "match":"Status van afspraak: .*",
              "replace":""
            },
            {
              "field":"summary",
              "match":"Wie: .*",
              "replace":""
            },
            {
              "field":"summary",
              "match":"Wanneer: ",
              "replace":""
            },
            {
              "field":"summary",
              "match":"Waar:",
              "replace":"<br/>Waar:"
            },
            {
              "field":"summary",
              "match":"Beschrijving van afspraak: .*",
              "replace":""
            },
            {
              "field":"summary",
              "match":"\n",
              "replace":"",
              "globalmatch":true,
              "multilinematch":true
            },
            {
              "field":"summary",
              "match":"\r",
              "replace":"",
              "globalmatch":true,
              "multilinematch":true
            },
            {
              "field":"summary",
              "match":"CET",
              "replace":"",
              "globalmatch":true
            },
            {
              "field":"summary",
              "match":"(\\d)\\.",
              "replace":"$1",
              "globalmatch":true
            },
            {
              "field":"title",
              "match":"Verk: ",
              "replace":""
            },
            {
              "field":"summary",
              "match":"CEST",
              "replace":"",
              "globalmatch":true
            },
            {
              "field":"summary",
              "match":"<br>",
              "replace":" ",
              "globalmatch":true,
              "multilinematch":true
            },
            {
              "field":"summary",
              "match":"&nbsp;",
              "replace":"",
              "globalmatch":true
            }
          ]
        }
      },
      {
        "type":"Rename",
        "id":3,
        "conf":{
          "rules":[
            {
              "field":"content",
              "op":"copy",
              "newval":"summary"
            }
          ]
        }
      },
      {
        "type":"Output",
        "id":4,
        "conf":{

        }
      }
    ],
    "wires":[
      {
        "id":0,
        "src":{
          "module":1
        },
        "tgt":{
          "module":2
        }
      },
      {
        "id":1,
        "src":{
          "module":2
        },
        "tgt":{
          "module":3
        }
      },
      {
        "id":2,
        "src":{
          "module":3
        },
        "tgt":{
          "module":4
        }
      }
    ]
  }
}